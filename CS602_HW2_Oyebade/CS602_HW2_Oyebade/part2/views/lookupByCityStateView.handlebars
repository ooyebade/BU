<b>Results for {{cityVar}}, {{stateVar}}...</b>
<p></p>
<table>
<tr>
    <th>ZipCode</th><th>Population</th>
    {{#each data}}
        <tr>
            <!--create the link that takes from zip to population-->
            <td><a href="/pop/{{state}}">{{this.zip}}</a></td>
            <td align="right">{{this.pop}}</td>
        </tr>
    {{/each}}  
</tr>
</table>
